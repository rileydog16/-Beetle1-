            "maxLength": 15
          },
          "elements": [
            {
              "id": "rule_3",
              "name": "The Liverpool Rule",
              "kind": "Rule",
              "properties": {
                "description": "Anything but Weevil",
                "formula": {
                  "expr": "$item !== 'Weevil'"
                }
              },
              "elements": []
            }
          ]
        },
        {
          "id": "texttype_6",
          "name": "Cage Type Type",
          "kind": "TextType",
