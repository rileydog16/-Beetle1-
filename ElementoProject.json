{
  "id": "project_1",
  "name": "New Project",
  "kind": "Project",
  "properties": {},
  "elements": [
    {
      "id": "app1",
      "name": "New App",
      "kind": "App",
      "properties": {},
      "elements": [
        {
          "id": "functionimport_1",
          "name": "Func 1",
          "kind": "FunctionImport",
          "properties": {
            "source": "Func1.js",
            "exportName": "*"
          },
          "elements": []
        },
        {
          "id": "page_1",
          "name": "Main Page",
          "kind": "Page",
          "properties": {},
          "elements": [
            {
              "id": "text_1",
              "name": "Text 1",
              "kind": "Text",
              "properties": {
                "content": "Giant Green Beetles!!!",
                "color": "green",
                "fontSize": 55
              },
              "elements": []
            },
            {
              "id": "textinput_1",
              "name": "Beetle Species",
              "kind": "TextInput",
              "properties": {
                "dataType": {
                  "expr": "DataTypes_datatypes_1.SpeciesType"
                },
                "width": "100%"
              },
              "elements": []
            },
            {
              "id": "numberinput_1",
              "name": "Wingspan",
              "kind": "NumberInput",
              "properties": {
                "dataType": {
                  "expr": "DataTypes_datatypes_1.WingspanType"
                }
              },
              "elements": []
            },
            {
              "id": "selectinput_1",
              "name": "Cage Type",
              "kind": "SelectInput",
              "properties": {
                "dataType": {
                  "expr": "DataTypes_datatypes_1.CageTypeType"
                },
                "values": [
                  "None",
                  "Small",
                  "Large",
                  "Tiny",
                  "Open"
                ]
              },
              "elements": []
            },
            {
              "id": "button_1",
              "name": "Button 1",
              "kind": "Button",
              "properties": {
                "content": "Reset Species",
                "appearance": "outline",
                "action": {
                  "expr": "Reset(BeetleSpecies)"
                }
              },
              "elements": []
            },
            {
              "id": "truefalseinput_1",
              "name": "Can Fly",
              "kind": "TrueFalseInput",
              "properties": {
                "dataType": {
                  "expr": "DataTypes_datatypes_1.CanFlyType1"
                }
              },
              "elements": []
            },
            {
              "id": "text_2",
              "name": "Text 2",
              "kind": "Text",
              "properties": {
                "content": {
                  "expr": "`All about insects and other ${CanFly.value ? 'flying' : 'wiggly'} creatures`"
                }
              },
              "elements": []
            },
            {
              "id": "text_3",
              "name": "Text 3",
              "kind": "Text",
              "properties": {
                "content": {
                  "expr": "'You are ' + Func1.Message(4)"
                }
              },
              "elements": []
            },
            {
              "id": "form_1",
              "name": "Beetle Details",
              "kind": "Form",
              "properties": {
                "label": "Beetle Details",
                "keyAction": {
                  "expr": "Log('Key pressed', $key)"
                }
              },
              "elements": [
                {
                  "id": "text_4",
                  "name": "Text 4",
                  "kind": "Text",
                  "properties": {
                    "content": "Tell us about your beetle"
                  },
                  "elements": []
                },
                {
                  "id": "textinput_2",
                  "name": "Species",
                  "kind": "TextInput",
                  "properties": {},
                  "elements": []
                },
                {
                  "id": "numberinput_2",
                  "name": "Wingspan",
                  "kind": "NumberInput",
                  "properties": {
                    "initialValue": 21
                  },
                  "elements": []
                },
                {
                  "id": "truefalseinput_2",
                  "name": "IsGreen",
                  "kind": "TrueFalseInput",
                  "properties": {},
                  "elements": []
                },
                {
                  "id": "text_6",
                  "name": "Form Summary",
                  "kind": "Text",
                  "properties": {
                    "content": {
                      "expr": "\"Your beetle is a \" + $form.value.Species + \" modified: \" + $form.modified + Species.modified + Wingspan.modified + IsGreen.modified"
                    }
                  },
                  "elements": []
                },
                {
                  "id": "button_2",
                  "name": "Button 2",
                  "kind": "Button",
                  "properties": {
                    "content": "Reset",
                    "appearance": "outline",
                    "action": {
                      "expr": "$form.Reset()"
                    }
                  },
                  "elements": []
                }
              ]
            },
            {
              "id": "text_5",
              "name": "Beetle Details Summary",
              "kind": "Text",
              "properties": {
                "content": {
                  "expr": "BeetleDetails"
                }
              },
              "elements": []
            },
            {
              "id": "text_7",
              "name": "Beetle Details Updates",
              "kind": "Text",
              "properties": {
                "content": {
                  "expr": "BeetleDetails.updates"
                }
              },
              "elements": []
            },
            {
              "id": "image_1",
              "name": "Beetle Image",
              "kind": "Image",
              "properties": {
                "source": "Tansy_Beetle.jpg",
                "width": "50%"
              },
              "elements": []
            },
            {
              "id": "image_2",
              "name": "Gazelle Image",
              "kind": "Image",
              "properties": {
                "source": "GazellePicStd.webp"
              },
              "elements": []
            }
          ]
        }
      ]
    },
